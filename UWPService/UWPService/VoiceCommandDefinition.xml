<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="fr-FR" Name="UniversalAppCommandSet_fr-FR">
    <CommandPrefix> Exam UWP </CommandPrefix>
    <Example> Dites à Cortana quelle page vous souhaitez </Example>
    <Command Name="showConversation">
      <Example>  Montre moi les factures</Example>
      <ListenFor > [Montre] [moi] [les] factures </ListenFor>
      <Feedback> Voici les factures </Feedback>
      <Navigate Target="Factures.xaml"/>
    </Command>
  </CommandSet>
</VoiceCommands>